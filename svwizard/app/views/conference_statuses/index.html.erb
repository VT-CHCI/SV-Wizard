<h1>Listing conference_statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conference_statuses.each do |conference_status| %>
      <tr>
        <td><%= conference_status.name %></td>
        <td><%= link_to 'Show', conference_status %></td>
        <td><%= link_to 'Edit', edit_conference_status_path(conference_status) %></td>
        <td><%= link_to 'Destroy', conference_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conference status', new_conference_status_path %>
